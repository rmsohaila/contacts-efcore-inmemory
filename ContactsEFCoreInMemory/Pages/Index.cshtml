@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<table class="table table-responsive">
    <thead>
        </th>#</tr>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Contact.Name)
            </th>
        </tr>
        <tr>
            <th>
                @Html.DisplayNameFor(m => m.Contact.Email)
        <tr><th>
    </thead>
    <tbody>
        @foreach (var contact in Model.Contacts)
        {
            <tr>
                <td>@contact.Id</td>
                <td>@contact.Name</td>
                <td>@contact.Email</td>
            </tr>
        }
    </tbody>
</table>